{
    // A unique model name. Generated haiku will be saved in $name.csv
    "name": "markov-character",
    // The type of model.
    "type": "markov",
    // Whether to use "words" tokenization, or "characters".
    "tokenization": "characters",
    // If set, seed the random number generator with a specific uint32_t seed. Otherwise a random one will be used and saved.
    "seed": null,
    // Whether to add ^...$ tags to the beginning and end of each haiku.
    "tags": true,
    // The Markov order.
    "order": 5,
    // The maximum number of tokens to generate per haiku. If tags are used, stop when you hit the first end tag.
    "max_tokens": 70,
    // The token sequence to to seed the generated haiku with.
    // A list of characters or strings based on the value of "tokenization".
    "seed_tokens": [
        "^",
        " ",
        "c",
        "h",
        "e",
        "r",
        "r",
        "y"
    ],
    // The number of haiku to generate.
    "number": 10
}
